        </record>

        <record id="group_gestore_agenzia" model="res.groups">
            <field name="name">Gestore</field>
            <field name="category_id" ref="Real_Estate_Project.module_category_agenzia"/>
            <field name="implied_ids" eval="[(4, ref('Real_Estate_Project.group_agente_agenzia'))]"/>
